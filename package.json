{
  "name": "Earn-A-Bike",
  "version": "0.2.0",
  "productName": "Earn-A-Bike Shop Manager",
  "description": "Earn-A-Bike Shop Manager",
  "author": "Halsey Vandenberg",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "build": "webpack --progress --config webpack.build.config.js",
    "dev": "webpack-dev-server --progress --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint -c ./.eslintrc.dev src --ext .jsx,.js",
    "lint:stylus": "find ./src/styles -type f -iname \"*.styl\" -print0 | xargs -0 -n1 ./node_modules/.bin/stylint",
    "package-mac": "electron-packager ./ Earn-A-Bike --platform=darwin --arch=x64 --overwrite --out=./builds --icon=src/assets/images/icon.icns",
    "package/win": "electron-packager ./ Earn-A-Bike --platform=win32 --arch=ia32 --overwrite --out=./builds --icon=src/assets/images/icon.ico --asar=true",
    "prebuild": "rimraf ./dist",
    "prod": "electron . --noDevServer",
    "testDev": "electron ."
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "classnames": "^2.2.5",
    "connected-react-router": "^4.2.1",
    "css-loader": "^0.26.2",
    "deep-equal": "^1.0.1",
    "electron": "^1.6.1",
    "electron-json-config": "^1.4.0",
    "electron-packager": "^8.7.2",
    "eslint": "^4.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "jsonfile": "^3.0.1",
    "moment": "^2.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "stylint": "^1.5.9",
    "stylint-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
